{
  "name": "worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "build": "wrangler deploy --dry-run --outdir=../dist/backend",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "cf-typegen": "wrangler types",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "check": "tsc --noEmit",
    "backfill:reddit": "tsx src/bin/backfill-reddit.ts",
    "backfill:discord": "tsx src/bin/backfill-discord.ts"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.0",
    "@cloudflare/workers-types": "^4.20240729.0",
    "@octokit/types": "^16.0.0",
    "@octokit/webhooks-types": "^7.6.1",
    "@types/express": "^5.0.0",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-istanbul": "^3.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "express": "^5.0.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.2",
    "vite-node": "^5.0.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "wrangler": "^4.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.633.0",
    "@influxdata/influxdb-client": "^1.34.0",
    "@octokit/auth-app": "^8.0.0",
    "@octokit/graphql": "^9.0.0",
    "fetch-retry": "^6.0.0",
    "fflate": "^0.8.2",
    "itty-router": "^5.0.18",
    "luxon": "^3.5.0",
    "p-limit": "^7.0.0"
  },
  "volta": {
    "node": "24.13.0"
  }
}
