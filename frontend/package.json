{
  "name": "data.immich.app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite dev",
    "build": "npm run sync && vite build",
    "preview": "vite preview",
    "check": "npm run sync && svelte-check --tsconfig ./tsconfig.json",
    "sync": "svelte-kit sync",
    "test": "vitest",
    "lint": "prettier --check . && eslint . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.13.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/eslint": "^9.0.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.14.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "^16.0.0",
    "postcss": "^8.4.39",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.1.5",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "type": "module",
  "dependencies": {
    "@immich/ui": "^0.17.1",
    "@mdi/js": "^7.4.47",
    "luxon": "^3.5.0",
    "uplot": "^1.6.31"
  }
}
